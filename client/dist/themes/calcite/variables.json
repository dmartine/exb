{
  "typography": {
    "fontFamilyBase": "'Avenir Next W01','Avenir Next', Avenir,'Helvetica Neue',Helvetica,Arial,sans-serif",
    "fontSizeRoot": "100%",
    "fontSizeBase": "0.875rem",
    "fontSizeLg": "1.125rem",
    "fontSizeSm": "0.75rem",
    "fontWeightLight": 300,
    "fontWeightNormal": 400,
    "fontWeightBold": 700,
    "fontWeightBase": 400,
    "lineHeightBase": 1.5,
    "lineHeightLg": 1.35,
    "lineHeightSm": 1.5,
    "headings": {
      "h1FontSize": "2.40307rem",
      "h2FontSize": "1.9994rem",
      "h3FontSize": "1.69949rem",
      "h4FontSize": "1.414rem",
      "h5FontSize": "1.2019rem",
      "h6FontSize": "1rem",
      "fontFamily": "inherit",
      "fontWeight": 400,
      "lineHeight": 1.35,
      "color": "inherit",
      "marginBottom": "1.5rem"
    }
  },
  "colors": {
    "grays": {
      "gray100": "#f8f8f8",
      "gray200": "#efefef",
      "gray300": "#ccc",
      "gray400": "#a9a9a9",
      "gray500": "#959595",
      "gray600": "#828282",
      "gray700": "#6e6e6e",
      "gray800": "#595959",
      "gray900": "#4c4c4c"
    },
    "greens": {
      "green100": "#cfedcc",
      "green200": "#96ce96",
      "green300": "#7dc17d",
      "green400": "#35ac46",
      "green500": "#2b622b"
    },
    "reds": {
      "red100": "#f3ded7",
      "red200": "#e4a793",
      "red300": "#c86a4a",
      "red400": "#c6542d",
      "red500": "#ff2f48"
    },
    "yellows": {
      "yellow100": "#f3edc7",
      "yellow200": "#efe397",
      "yellow300": "#e4d154",
      "yellow400": "#ffec4f",
      "yellow500": "#d9bf2b"
    },
    "oranges": {
      "orange100": "#fbe1c2",
      "orange200": "#eda959",
      "orange300": "#f89927",
      "orange400": "#f36f20",
      "orange500": "#da4d1e"
    },
    "blues": {
      "blue100": "#e2f1fb",
      "blue200": "#aadbfa",
      "blue300": "#90cdf2",
      "blue400": "#0079c1",
      "blue500": "#005e95"
    },
    "purples": {
      "purple100": "#e5e1ef",
      "purple200": "#dfcce3",
      "purple300": "#b279b4",
      "purple400": "#8f499c",
      "purple500": "#6b1978"
    },
    "white": "#fff",
    "black": "#323232",
    "blue": "#0079c1",
    "purple": "#6b1978",
    "red": "#de2900",
    "orange": "#f36f20",
    "yellow": "#e4d154",
    "green": "#35ac46",
    "gray": "#959595",
    "grayDark": "#828282",
    "primary": "#0079c1",
    "secondary": "#efefef",
    "success": "#31872e",
    "info": "#aadbfa",
    "warning": "#e4d154",
    "danger": "#de2900",
    "light": "#f8f8f8",
    "dark": "#323232"
  },
  "border": {
    "color": "#a9a9a9",
    "width": "1px"
  },
  "borderRadiuses": {
    "default": "0",
    "lg": "3px",
    "sm": "0"
  },
  "boxShadows": {
    "default": "0 0 16px 0 rgba(0, 0, 0, 0.05)",
    "lg": "0 0 16px 0 rgba(0, 0, 0, 0.05)",
    "sm": "none"
  },
  "body": {
    "bg": "#fff",
    "color": "#4c4c4c"
  },
  "header": {
    "bg": "#0079c1",
    "color": "#fff"
  },
  "link": {
    "color": "#0079c1",
    "hover": {
      "color": "#005e95"
    }
  },
  "components": {
    "alert": {
      "paddingY": "0.75rem",
      "paddingX": "0.75rem",
      "marginBottom": "0.625rem",
      "borderRadius": "0",
      "linkFontWeight": 300,
      "borderWidth": "1px",
      "bgLevel": "-10",
      "borderLevel": "0",
      "colorLevel": "6"
    },
    "badge": {
      "fontSize": "0.875em",
      "fontWeight": 400,
      "paddingX": "0.5em",
      "paddingY": "0.25em",
      "borderRadius": "3px",
      "pill": {
        "paddingX": "0.5em",
        "borderRadius": "10rem"
      }
    },
    "breadcrumb": {
      "bg": "transparent",
      "activeColor": "#6e6e6e",
      "paddingY": "0",
      "paddingX": "0",
      "marginBottom": "0",
      "item": {
        "color": "#6e6e6e",
        "padding": "0.5rem"
      },
      "divider": {
        "color": "#6e6e6e"
      }
    },
    "button": {
      "outlineBorder": "auto",
      "sizes": {
        "default": {
          "fontSize": "0.9375rem",
          "lineHeight": 1.5,
          "paddingX": "1rem",
          "paddingY": "0.375rem",
          "borderRadius": "0"
        },
        "sm": {
          "fontSize": "0.875rem",
          "lineHeight": 1.5,
          "paddingX": "0.75rem",
          "paddingY": "0.28125rem",
          "borderRadius": "0"
        },
        "lg": {
          "fontSize": "1rem",
          "lineHeight": 1.5,
          "paddingX": "1rem",
          "paddingY": "0.5rem",
          "borderRadius": "0"
        }
      },
      "variants": {
        "primary": {
          "normal": "#0079c1",
          "hover": "#005e95"
        },
        "secondary": {
          "normal": "#ccc",
          "hover": "#a9a9a9"
        },
        "dark": {
          "normal": "#323232",
          "hover": "#000"
        },
        "light": {
          "normal": "#f8f8f8",
          "hover": "#efefef"
        }
      }
    },
    "card": {
      "border": {
        "width": 0
      }
    },
    "dropdown": {
      "paddingY": "0",
      "border": {
        "color": "#efefef",
        "width": "1px"
      },
      "link": {
        "color": "#6e6e6e",
        "hoverColor": "#6e6e6e",
        "hoverBg": "#f8f8f8",
        "activeColor": "#6e6e6e",
        "activeBg":  "#f8f8f8"
      },
      "item": {
        "paddingY": "0.5rem",
        "paddingX": "0.5rem"
      }
    },
    "input": {
      "sizes": {
        "default": {
          "fontSize": "0.9375rem",
          "borderRadius": "0",
          "lineHeight": "1.25rem",
          "height": "2.25rem",
          "paddingY": "0",
          "paddingX": "0.3rem"
        },
        "sm": {
          "borderRadius": "0",
          "height": "auto"
        },
        "lg": {
          "borderRadius": "0",
          "height": "auto"
        }
      },
      "switch": {
        "width": "38px",
        "height": "22px",
        "bg": "#f4f4f4",
        "slider": {
          "gap": "0",
          "color": "#fff"
        }
      },
      "slider": {
        "sizes": {
          "default": {
            "height": "0.125rem"
          },
          "sm": {
            "height": "0.125rem"
          },
          "lg": {
            "height": "0.125rem"
          }
        },
        "bg": "#ccc",
        "thumb": {
          "sizes": {
            "defaultSize": "1.125rem",
            "smSize": "1.125rem",
            "lgSize": "1.125rem"
          },
          "bg": "#fff",
          "borderColor": "#959595"
        }
      }
    },
    "listGroup": {
      "border": {
        "color": "#efefef"
      },
      "borderRadius": 0,
      "item": {
        "paddingY": "0.5rem",
        "paddingX": "0.5rem"
      },
      "active": {
        "borderColor": "#0079c1"
      }
    },
    "modal": {
      "innerPadding": "0 1.5rem 1.5rem",
      "title": {
        "lineHeight": 1
      },
      "backdrop": {
        "bg": "#000",
        "opacity": 0.75
      },
      "content": {
        "borderRadius": 0
      },
      "header": {
        "border": {
          "width": 0
        },
        "paddingY": "1.5rem",
        "paddingX": "1.5rem"
      }
    },
    "nav": {
      "link": {
        "paddingY": "0.375rem",
        "paddingX": "0.75rem"
      },
      "tabs": {
        "gutter": "-1px",
        "link": {
          "color": "#0079c1",
          "hover": {
            "bg": "#fcfcfc",
            "borderColor": "#ccc"
          },
          "active": {
            "borderColor": "#ccc #ccc #fff"
          }
        }
      }
    },
    "navbar": {
      "fontSize": "0.9375rem",
      "toggler": {
        "borderRadius": 0
      },
      "themes": {
        "dark": {
          "toggler": {
            "borderColor": "transparent"
          }
        },
        "light": {
          "toggler": {
            "borderColor": "transparent"
          }
        }
      }
    },
    "pagination": {
      "color": "#0079c1",
      "bg": "transparent",
      "border": {
        "width": 0
      },
      "lineHeight": 1.5,
      "gutter": "3px",
      "hover": {
        "bg": "transparent",
        "borderColor": "transparent"
      },
      "disabled": {
        "color": "#aadbfa",
        "bg": "transparent",
        "borderColor": "transparent"
      },
      "sizes": {
        "default": {
          "paddingY": "0.375rem",
          "paddingX": "1rem"
        }
      },
      "item": {
        "borderRadius": 0 
      }
    },
    "popover": {
      "borderRadius": 0,
      "boxShadow": "0 0 0 1px rgba(0,0,0,0.1), 0 0 16px 0 rgba(0,0,0,0.05)",
      "border": {
        "width": 0
      },
      "header": {
        "bg": "#f8f8f8",
        "paddingY": "1rem",
        "paddingX": "1rem"
      },
      "body": {
        "color": "#6e6e6e",
        "paddingY": "1rem",
        "paddingX": "1rem"
      },
      "arrow": {
        "outerColor": "#ccc"
      }
    },
    "progress": {
      "borderRadius": 0
    },
    "tooltip": {
      "bg": "#000",
      "color": "#fff",
      "opacity": 0.75,
      "paddingY": "0.375rem",
      "paddingX": "0.75rem",
      "arrow": {
        "color": "#000"
      }
    }
  },
  "helpers": {
    "typography": {
      "display": {
        "1": {
          "fontSize": "2.40307rem",
          "fontWeight": 400
        },
        "2": {
          "fontSize": "1.9994rem",
          "fontWeight": 400
        },
        "3": {
          "fontSize": "1.69949rem",
          "fontWeight": 400
        },
        "4": {
          "fontSize": "1.414rem",
          "fontWeight": 400
        }
      }
    }
  }
}