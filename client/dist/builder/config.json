{
  "pages": {
    "default": {
      "label": "config page",
      "layout": {
        "LARGE": "config-page-layout-large"
      },
      "header": true,
      "isDefault": true,
      "backgroundColor": "#eef0f3"
    },
    "template": {
      "label": "choose template",
      "layout": {
        "LARGE": "choose-template-layout-large"
      },
      "header": false
    }
  },
  "widgets": {
    "header": {
      "uri": "builder/widgets/header/"
    },
    "choose-template": {
      "uri": "builder/widgets/choose-template/"
    },
    "setting-navigator": {
      "uri": "builder/widgets/setting-navigator/",
      "config": {
        "sectionId": "opts-section"
      }
    },
    "toc": {
      "uri": "builder/widgets/toc/"
    },
    "app-loader": {
      "uri": "builder/widgets/app-loader/"
    },
    "app-toolbar": {
      "uri": "builder/widgets/app-toolbar/"
    },
    "builder-entry": {
      "uri": "builder/widgets/builder-entry/"
    },
    "data-source-setting": {
      "uri": "builder/widgets/data-source-setting/"
    },
    "theme-setting": {
      "uri": "builder/widgets/theme-setting/"
    },
    "dynamic-setting": {
      "uri": "builder/widgets/dynamic-setting/"
    },
    "left-sidebar": {
      "uri": "widgets/layout/sidebar-horizontal/",
      "config": {
        "collapseSide": "FIRST",
        "resizable": false,
        "overlay": false,
        "size": "260px",
        "toggleBtn": {
          "icon": "arrow-left-14",
          "visible": true,
          "offsetX": 15,
          "offsetY": 0,
          "position": "CENTER",
          "iconSize": 14,
          "width": 15,
          "height": 60,
          "color": {
            "normal": {
              "icon": {
                "useTheme": true,
                "color": "colors.grays.gray400"
              },
              "bg": {
                "useTheme": true,
                "color": "colors.grays.gray100"
              }
            },
            "hover": {
              "icon": {
                "useTheme": true,
                "color": "colors.black"
              }
            }
          },
          "expandStyle": {
            "style": {
              "borderRadius": "0 92px 92px 0"
            }
          },
          "collapseStyle": {
            "style": {
              "borderRadius": "0 92px 92px 0"
            }
          }
        },
        "divider": {
          "visible": true,
          "lineStyle": {
            "type": "solid",
            "color": "#000",
            "width": {
              "distance": 1,
              "unit": "px"
            }
          }
        }
      },
      "layouts": {
        "FIRST": {
          "LARGE": "left-sidebar-left-layout"
        },
        "SECOND": {
          "LARGE": "left-sidebar-right-layout"
        }
      }
    },
    "right-sidebar": {
      "uri": "widgets/layout/sidebar-horizontal/",
      "config": {
        "collapseSide": "SECOND",
        "resizable": false,
        "overlay": false,
        "size": "260px",
        "toggleBtn": {
          "icon": "arrow-right-14",
          "visible": true,
          "offsetX": -15,
          "offsetY": 0,
          "position": "CENTER",
          "iconSize": 14,
          "width": 15,
          "height": 60,
          "color": {
            "normal": {
              "icon": {
                "useTheme": true,
                "color": "colors.grays.gray400"
              },
              "bg": {
                "useTheme": true,
                "color": "colors.grays.gray100"
              }
            },
            "hover": {
              "icon": {
                "useTheme": true,
                "color": "colors.black"
              }
            }
          },
          "expandStyle": {
            "style": {
              "borderRadius": "92px 0 0 92px"
            }
          },
          "collapseStyle": {
            "style": {
              "borderRadius": "92px 0 0 92px"
            }
          }
        },
        "divider": {
          "visible": true,
          "lineStyle": {
            "type": "solid",
            "color": "#000",
            "width": {
              "distance": 1,
              "unit": "px"
            }
          }
        }
      },
      "layouts": {
        "FIRST": {
          "LARGE": "right-sidebar-left-layout"
        },
        "SECOND": {
          "LARGE": "right-sidebar-right-layout"
        }
      }
    }
  },
  "preloadWidgets": [
    "header",
    "setting-navigator",
    "toc",
    "app-toolbar",
    "builder-entry",
    "data-source-setting",
    "theme-setting",
    "dynamic-setting",
    "left-sidebar",
    "right-sidebar",
    "app-loader",
    "choose-template"
  ],
  "sections": {
    "opts-section": {
      "views": [
        "toc-view",
        "data-source-view",
        "theme-view"
