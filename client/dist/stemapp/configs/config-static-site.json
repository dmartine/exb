  "widgets": {
    "header-logo": {
      "id": "header-logo",
      "uri": "widgets/common/image/",
      "config": {
        "src": "assets/img/Logo.svg",
        "alt": "Radar Experience",
        "height": 30,
        "width": 37
      }
    },
    "header-title-text": {
      "id": "header-title-text",
      "uri": "widgets/common/text/",
      "config": {
        "text": "<span style='font-size:20px'>Water</span> "
