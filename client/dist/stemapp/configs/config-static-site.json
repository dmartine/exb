    "main--splash-image": {
      "id": "main--splash-image",
      "uri": "widgets/common/image/",
      "config": {
        "src": "https://www.esri.com/content/esri-sites/en-us/industries/water/overview/_jcr_content/par/grid_container.img.jpg/1532973140201.jpg",
        "alt": "",
        "cover": true,
        "height": 680
