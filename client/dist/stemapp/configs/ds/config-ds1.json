{
  "pages": {
    "default": {
      "id": "default",
      "label": "page 1",
      "type": "NORMAL",
      "layout": {"LARGE": "page-default-layout"},
      "isDefault": true
    }
  },
  "theme": "themes/default",
  "widgets": {
    "fl-widget1": {
      "uri": "widgets/samples/data-sources/use-feature-layer-widget/",
      "useDataSources": [
        {
          "dataSourceId": "fl-1",
          "fields": ["CITY_NAME"]
        }
      ]
    },
    "fl-widget2": {
      "uri": "widgets/samples/data-sources/use-feature-layer-with-mapping-widget/",
      "useDataSources": [
        {
          "dataSourceId": "fl-2",
          "fields": [
            "jimu_field_1"
          ]
        }
      ],
      "config": {
        "fieldName": "jimu_field_1"
      }
    },
    "show-id": {
      "uri": "widgets/samples/data-action/show-record-id-widget"
    }
  },
  "layouts": {
    "page-default-layout": {
      "type": "FIXED",
      "order": ["1", "2"],
      "content": {
        "1": {
          "type": "WIDGET",
          "widgetId": "fl-widget1",
          "bbox": {
            "left": 100,
            "width": 500,
            "top": 100
          }
        },
        "2": {
          "type": "WIDGET",
          "widgetId": "fl-widget2",
          "bbox": {
            "left": 800,
            "width": 500,
            "top": 100
          }
        }
      }
    }
  },
  "dataSources": {
    "fl-1": {
      "id": "fl-1",
      "label": "f",
      "type": "FEATURE_QUERY",
      "url": "https://sampleserver6.arcgisonline.com/arcgis/rest/services/SampleWorldCities/MapServer/0"
    },
    "fl-2": {
      "id": "fl-2",
      "label": "f2",
      "type": "FEATURE_QUERY",
      "url": "https://sampleserver6.arcgisonline.com/arcgis/rest/services/SampleWorldCities/MapServer/0",
      "schema": {
        "fields": {
          "jimu_field_1": {
            "jimuName": "jimu_field_1",
            "name": "CITY_NAME",
            "type": "STRING"
          }
        }
      }
    }
  }
}