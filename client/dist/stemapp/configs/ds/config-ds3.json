{
  "pages": {
    "default": {
      "id": "default",
      "label": "page 1",
      "layout": {"LARGE": "page-default-layout"},
      "widgets": [
        "search-github-widget"
      ],
      "isDefault": true
    }
  },
  "theme": "themes/default",
  "widgets": {
    "search-github-widget": {
      "id": "search-github-widget",
      "uri": "widgets/samples/data-sources/search-github-widget/",
      "useDataSources": [{
        "dataSourceId": "github-repo"
      }],
      "outputDataSources": ["github-repo-records"]
    },
    "show-github-repo-widget": {
      "id": "show-github-repo-widget",
      "uri": "widgets/samples/data-sources/show-github-repo-widget/",
      "useDataSources": [{
        "dataSourceId": "github-repo-records"
      }]
    }
  },
  "layouts": {
    "page-default-layout": {
      "type": "FIXED",
      "order": ["1", "2"],
      "content": {
        "1": {
          "bbox": {
            "left": 100,
            "top": 20,
            "width": 500,
            "height": 500
          },
          "id": "1",
          "type": "WIDGET",
          "widgetId": "search-github-widget"
        },
        "2": {
          "bbox": {
            "left": 700,
            "top": 20,
            "width": 500,
            "height": 500
          },
          "id": "2",
          "type": "WIDGET",
          "widgetId": "show-github-repo-widget"
        }
      }
    }
  },
  "dataSources": {
    "github-repo": {
      "id": "github-repo",
      "label": "github repo",
      "type": "GITHUB_REPOSITORY",
      "url": "https://api.github.com/search/repositories"
    },
    "github-repo-records": {
      "id": "github-repo-records",
      "label": "github repo records",
      "type": "SIMPLE_LOCAL",
      "isOutputFromWidget": true
    }
  }
}