{
  "name": "arcgis-map",
  "label": "Map",
  "type": "widget",
  "version": "1.0.0-beta",
  "exbVersion": "1.0.0-beta",
  "author": "Esri R&D Center Beijing",
  "description": "This is the widget used in developer guide",
  "copyright": "",
  "license": "http://www.apache.org/licenses/LICENSE-2.0",
  "publishMessages": [
    "EXTENT_CHANGE"
  ],
  "messageActions": [
    {
      "name": "displayFeatureSet",
      "label": "Display feature set",
      "uri": "message-actions/display-feature-set-action"
    },
    {
      "name": "panTo",
      "label": "Pan to",
      "uri": "message-actions/pan-to-action"
    },
    {
      "name": "zoomToFeature",
      "label": "Zoom to",
      "uri": "message-actions/zoom-to-feature-action"
    },
    {
      "name": "flash",
      "label": "Flash",
      "uri": "message-actions/flash-action",
      "settingUri": "message-actions/flash-action-setting"
    }
  ],
  "defaultSize": {
    "width": 400,
    "height": 400
  },
  "properties": {
    "hasLocale": false,
    "autoBindDataSource": false
  },
  "supportedLocales": [
    "en"
  ]
}