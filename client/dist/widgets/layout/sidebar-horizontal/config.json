  "overlay": false,
  "size": "300px",
  "divider": {
    "visible": true
  },
  "resizable": true,
  "toggleBtn": {
    "visible": true,
    "icon": "arrow-left-14",
    "offsetX": 15,
    "offsetY": 0,
    "position": "CENTER",
    "iconSize": 14,
    "width": 15,
    "height": 60,
    "color": {
      "normal": {
        "icon": {
          "useTheme": false,
          "color": "#FFFFFF"
        },
        "bg": {
          "useTheme": true,
