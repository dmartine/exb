        }
      },
      "hover": {
        "bg": {
          "useTheme": true,
          "color": "colors.indigos.indigo500"
        }
      }
    },
    "expandStyle": {
      "style": {
        "borderRadius": "0 92px 92px 0"
      }
    },
    "collapseStyle": {
      "style": {
        "borderRadius": "0 92px 92px 0"
      }
    }
  },
