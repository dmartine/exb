{
  "typography": {
    "fontFamilyBase": "Avenir Next W01",
    "fontSizeRoot": "100%",
    "fontSizeBase": "0.8125rem",
    "fontSizeLg": "1rem",
    "fontSizeSm": "0.75rem",
    "fontWeightLight": 300,
    "fontWeightNormal": 300,
    "fontWeightBold": 400,
    "fontWeightBase": 300,
    "lineHeightBase": 1.5,
    "lineHeightLg": 1.5,
    "lineHeightSm": 1,
    "headings": {
      "h1FontSize": "2rem",
      "h2FontSize": "1.5rem",
      "h3FontSize": "1.125rem",
      "h4FontSize": "1rem",
      "h5FontSize": "0.875rem",
      "h6FontSize": "0.8125rem",
      "fontFamily": "inherit",
      "fontWeight": 300,
      "lineHeight": 1.25,
      "color": "inherit",
      "marginBottom": "0.5rem"
    },
    "paragraph": {
      "marginBottom": "1rem"
    },
    "label": {
      "marginBottom": "0.5rem"
    },
    "hr": {
      "marginY": "1rem",
      "borderColor": "rgba(255, 255, 255, 0.1)",
      "borderWidth": "1px"
    },
    "small": {
      "fontSize": "80%"
    },
    "mark": {
      "padding": "0.2em",
      "bg": "#fcf8e3"
    }
  },
  "colors": {
    "grays": {
      "gray100": "#232323",
